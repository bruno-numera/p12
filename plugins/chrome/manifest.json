{
  "name": "Message Timer",
  "version": "1.0",
  "description": "Times how long it takes to send a message to a content script and back.",
  "permissions": [
  	"tabs",
  	"http://*/*",
  	"https://*/*"
  	],
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "css" : ["style.css"],
      "js": ["jquery.js", "page.js"]
    }
  ],
  "background_page": "background.html"
  
}